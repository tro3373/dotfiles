#!/bin/bash

install_msys() {
    not_supported
}
install_cygwin() {
    not_supported
}
install_ubuntu() {
    _install_common
}
install_redhat() {
    _install_common
}
install_mac() {
    _install_common
}

_install_common() {
    mexe git clone https://github.com/junegunn/fzf.git ~/.fzf
    mexe ~/.fzf/install
}

