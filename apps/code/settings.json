{
  "files.hotExit": "onExit",
  "window.restoreWindows": "all",
  "window.openFoldersInNewWindow": "on",
  // "workbench.colorTheme": "Apprentice",
  "workbench.colorTheme": "One Dark Pro Darker",
  // "workbench.colorTheme": "One Dark Pro Darker",
  // "workbench.colorTheme": "Atom One Dark",
  // "workbench.colorTheme": "Apprentice",
  // "workbench.colorTheme": "GitHub Dark",
  "workbench.iconTheme": "vscode-icons",
  "editor.mouseWheelZoom": true,
  "editor.fontFamily": "Osaka-Mono, 'UDEV Gothic JPDOC', 'HackGen Console', Cica-Regular, 'Ricty Diminished Discord', 'Ricty Diminished', 'Myrica M', Consolas, 'Courier New', monospace",
  "editor.fontSize": 18,
  "editor.lineHeight": 20,
  "editor.wordWrap": "on",
  "editor.cursorStyle": "block",
  "editor.fontLigatures": false,
  "editor.insertSpaces": true,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?、。　",
  "editor.formatOnType": true, // 入力後の自動フォーマットは無効
  "editor.formatOnSave": true, // Prettier を実行
  "editor.formatOnSaveMode": "file",
  "editor.formatOnPaste": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  // "eslint.autoFixOnSave": true,
  // "tslint.autoFixOnSave": true,
  // "prettier.eslintIntegration": true,
  // "prettier.tslintIntegration": true,
  // "prettier.singleQuote": true,
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "[javascript]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // }
  // ===================================
  // "eslint.validate": ["javascript", "javascriptreact"],
  // "eslint.autoFixOnSave": true,
  // ===================================
  "editor.renderWhitespace": "boundary",
  "editor.inlineSuggest.enabled": true,
  "editor.stickyScroll.enabled": true,
  "files.trimFinalNewlines": false,
  "files.trimTrailingWhitespace": true,
  "files.autoGuessEncoding": true,
  "bookmarks.treeview.visible": true,
  "vim.useSystemClipboard": true,
  "vim.cmdLineInitialColon": true,
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.visualstar": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-d>": true,
    // "<C-u>": true,
    "<C-j>": false,
    "<C-p>": false
  },
  // "vim.foldFix": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // VSCodeでVim拡張使っているとコードが勝手に領域展開しちゃう問題に対処する
    // https://zenn.dev/kenfdev/articles/3637e70b450d56
    {
      "before": ["<C-u>"],
      "after": ["H", "z", "z"]
    },
    {
      "before": ["<C-d>"],
      "after": ["L", "z", "z"]
    },
    // https://fe-notes.work/posts/20200708_vsvim01/
    {
      "before": ["u"],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": ["<C-r>"],
      "commands": [
        {
          "command": "redo"
        }
      ]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    },
    {
      "before": ["*"],
      "after": ["*", "z", "z"]
    },
    {
      "before": ["\""],
      "after": ["*", "z", "z"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<Leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": ["<Leader>", "q"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    {
      "before": ["<Leader>", "x"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatDocument",
          "args": []
        }
      ]
    },
    {
      "before": ["<Leader>", "c"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": ["<Leader>", "'"],
      "after": ["$"]
    },
    {
      "before": ["<Leader>", "m"],
      "after": ["%"]
    }
    // {
    //   "before": ["s", "p"],
    //   "after": [],
    //   "commands": [
    //     {
    //       "command": "workbench.action.previousEditor",
    //       "args": []
    //     }
    //   ]
    // },
    // {
    //   "before": ["s", "n"],
    //   "after": [],
    //   "commands": [
    //     {
    //       "command": "workbench.action.nextEditor",
    //       "args": []
    //     }
    //   ]
    // },
    // {
    //   "before": ["s", "t"],
    //   "after": [],
    //   "commands": [
    //     {
    //       // "command": "explorer.newFile",
    //       // "command": "workbench.action.files.newUntitledFile",
    //       "command": "workbench.action.quickOpen",
    //       "args": []
    //     }
    //   ]
    // },
    // {
    //   "before": ["s", "d"],
    //   "commands": ["workbench.view.explorer", "revealInExplorer"]
    // }
  ],
  "vim.commandLineModeKeyBindingsNonRecursive": [
    {
      "before": ["s", "<Leader>"],
      "after": ["s", "/", "/", "/", "g", "<Left>", "<Left>"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": [";", ";"],
      "commands": ["editor.action.triggerSuggest"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "c"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      // 選択範囲をコピーして検索モードに入る
      "before": ["\""],
      "after": ["y", "/", "<C-r>", "\"", "<CR>"],
      "commands": []
    }
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.svn/**": true
  },
  "scm.alwaysShowProviders": true,
  "search.exclude": {
    "**/bower_components": true,
    "**/vendor": true,
    "**/env": true,
    "tags": true,
    "**/.svn": true,
    "**/.git": true,
    "**/.DS_Store": true
  },
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "ArchLinux (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "ArchLinux"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "ArchLinux (WSL)",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "hediet.vscode-drawio.resizeImages": null,
  // Markdown
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": false
  },
  "markdownlint.config": {
    "default": true,
    "MD007": { "indent": 4 }
  },
  // "java.format.settings.profile": "GoogleStyle",
  // "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.format.settings.google.extra": "--aosp", // For 4 instead of 2 spaces!
  "[java]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "josevseb.google-java-format-for-vs-code"
  },
  // Javascript
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Go
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  // Python
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "isort.args": ["--profile", "black"],
  "flake8.args": ["--max-line-length=120", "--ignore=W503,E501,E203"],
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "none"
  },
  "mypy-type-checker.args": ["--ignore-missing-imports"],
  "hediet.vscode-drawio.theme": "sketch",
  "workbench.startupEditor": "none",
  "github.copilot.editor.enableAutoCompletions": true,
  "remote.downloadExtensionsLocally": true,
  "shellformat.flag": "",
  "workbench.activityBar.orientation": "vertical",
  "makefile.configureOnOpen": false,
  "cursor.cpp.disabledLanguages": ["plaintext", "scminput"],
  "terminal.integrated.sendKeybindingsToShell": true,
  "go.toolsManagement.autoUpdate": true,
  "vsicons.dontShowNewVersionMessage": true,
  "geminicodeassist.inlineSuggestions.enableAuto": true,
  "geminicodeassist.project": "",
  "geminicodeassist.enableTelemetry": false,
  "gitlens.telemetry.enabled": false,
  "print.folder.fileNames": true,
  "print.folder.include": [],
  "print.folder.exclude": [
    "{bin,obj,out}",
    "node_modules",
    "data",
    "**/*.{bin,exe,dll,hex,pdb,pdf,pfx,png,jpg,gif,bmp,suo,design}"
  ],
  "print.folder.maxLines": 1200,
  "print.folder.maxFiles": 30,
  "print.folder.includeFileList": false,
  "roo-cline.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show",
    "go test"
  ],
  "git.openRepositoryInParentFolders": "always"
}
