#!/bin/bash

current_dir=$(pwd)
script_dir=$(cd $(dirname $0); pwd)
template=$script_dir/lib/template.sh

main(){
    f=${1:-tmp.sh}
    if [[ -e $f ]]; then
        echo "Target name file is exist. name=$f"
        exit 1
    fi
    cp $template $current_dir/$f
    echo "File is created. $f"
}
main $*

