#!/usr/bin/env bash

_dump_certs() {
  keytool -keystore $keystore -export -alias $alias -rfc -file output.crt
}

# _dump_keys() {
#   keytool -importkeystore -srckeystore $keystore -destkeystore [target-keystore] -deststoretype PKCS12
# }

main() {
  keystore=$1
  alias=${2:-key0}
  if [[ ! -e $keystore ]]; then
    echo "Specify keytstore!" 1>&2
    exit 1
  fi
  _dump_certs
}
main "$@"
